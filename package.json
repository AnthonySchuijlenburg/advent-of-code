        "eslint": "^8.57.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^5.2.3",
        "eslint-plugin-promise": "^7.2.1",
        "eslint-plugin-react": "^7.37.4",
        "prettier": "^3.4.2",
        "typescript": "^5.7.3",
        "vitest": "^2.1.8"
    },
    "dependencies": {
        "mathjs": "^13.2.3",
